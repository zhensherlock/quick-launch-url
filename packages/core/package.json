{
  "name": "@quick-launch-url/core",
  "version": "0.0.0",
  "description": "One-click launch URL generator for protocol-based apps.",
  "type": "module",
  "author": "zhensherlock",
  "homepage": "https://github.com/zhensherlock/quick-launch-url/tree/master/apps/core#readme",
  "keywords": ["quick-launch", "quick-launch-url", "typescript"],
  "main": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "MIT",
  "files": ["dist", "LICENSE", "README.md"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhensherlock/quick-launch-url.git"
  },
  "scripts": {
    "clean:dist": "rimraf dist",
    "dev": "rolldown -c rolldown.config.ts --watch --no-clear-screen",
    "build:types": "tsc",
    "build": "npm run clean:dist && npm run build:types && rolldown -c rolldown.config.ts"
  },
  "bugs": {
    "url": "https://github.com/zhensherlock/quick-launch-url/issues"
  },
  "dependencies": {
    "@quick-launch-url/shared": "workspace:*"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.esm.js"
    },
    "./cherry-studio": {
      "types": "./dist/types/cherry-studio/index.d.ts",
      "import": "./dist/cherry-studio/index.js"
    },
    "./cursor": {
      "types": "./dist/types/cursor/index.d.ts",
      "import": "./dist/cursor/index.js"
    }
  }
}
